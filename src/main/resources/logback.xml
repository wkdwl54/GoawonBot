<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>

    <logger level="debug" name="me.kokona.discordbot">
        <appender-ref ref="STDOUT"/>
    </logger>
</configuration>